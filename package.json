{
  "name": "paginghelper",
  "version": "0.0.1",
  "description": "A utility for generating pagings with first/last page and ellipsis values.",
  "main": "src/PagingHelper.js",
  "scripts": {
    "test": "node_modules/.bin/jasmine",
    "validate": "node_modules/.bin/jshint src/",
    "pretest": "npm run validate"
  },
  "keywords": [
    "paging",
    "ellipsis",
    "page",
    "navigation"
  ],
  "devDependencies": {
    "jasmine": "^2.4.1",
    "jshint": "^2.9.3"
  },
  "author": "Lennart Pegel <github@justlep.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/justlep/paginghelper.git"
  },
  "license": "MIT",
  "jshintConfig": {
    "passfail": false,
    "maxerr": 100,
    "maxlen": 130,
    "browser": true,
    "node": true,
    "rhino": false,
    "couch": false,
    "wsh": true,
    "jquery": true,
    "prototypejs": false,
    "mootools": false,
    "dojo": false,
    "predef": [
      "require",
      "define"
    ],
    "debug": false,
    "devel": true,
    "strict": true,
    "globalstrict": false,
    "asi": true,
    "laxbreak": true,
    "bitwise": true,
    "boss": false,
    "curly": true,
    "eqeqeq": true,
    "eqnull": false,
    "evil": false,
    "expr": false,
    "forin": false,
    "immed": true,
    "latedef": true,
    "loopfunc": false,
    "noarg": true,
    "regexp": true,
    "regexdash": false,
    "scripturl": true,
    "shadow": false,
    "supernew": false,
    "undef": true,
    "newcap": true,
    "noempty": true,
    "nonew": true,
    "nomen": true,
    "onevar": false,
    "plusplus": false,
    "sub": false,
    "trailing": true,
    "white": false,
    "indent": 4
  }
}
